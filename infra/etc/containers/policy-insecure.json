{
    "default": [
        {
            "type": "insecureAcceptAnything"
        }
    ],
    "transports": {
        "docker": {
            "us-docker.pkg.dev/cordialsys/containers": [
                {
                    "type": "insecureAcceptAnything"
                }
            ],
            "registry.access.redhat.com": [
                {
                    "type": "signedBy",
                    "keyType": "GPGKeys",
                    "keyPaths": [
                        "/etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release",
                        "/etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-beta"
                    ]
                }
            ],
            "registry.redhat.io": [
                {
                    "type": "signedBy",
                    "keyType": "GPGKeys",
                    "keyPaths": [
                        "/etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release",
                        "/etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-beta"
                    ]
                }
            ]
        },
        "docker-daemon": {
            "": [
                {
                    "type": "insecureAcceptAnything"
                }
            ]
        }
    }
}